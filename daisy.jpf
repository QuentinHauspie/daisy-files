# set target application file and path to compiled and source code
target = DaisyTest
classpath = ./src
sourcepath = ./src

# Enumerate choices
cg.enumerate_random = true

# report output on error paths only
vm.tree_output=false
vm.path_output=true

# rich reporting
report.console.property_violation = error,trace,snapshot,output

# do not report depth limit hits
report.console.constraint =

report.console.show_steps = true

#search.depth_limit = 1000
